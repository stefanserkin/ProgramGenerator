<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>StagedMustHaveASession</fullName>
    <active>true</active>
    <description>Staged programs must have a session</description>
    <errorConditionFormula>AND(
  ISPICKVAL( Status__c, &quot;Staged&quot; ),
  ISBLANK( Session__c )
)</errorConditionFormula>
    <errorDisplayField>Session__c</errorDisplayField>
    <errorMessage>Staged programs must have a session</errorMessage>
</ValidationRule>
